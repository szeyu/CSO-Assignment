0002 7400                4     MAIN:   MOV     A,#00H
0004 F590                5             MOV     P1,A    ; Set Port 1 as output for Green Light and Red Light
0006 F5A0                6             MOV     P2,A    ; Set Port 2 as output for Yellow Light
0008 F5B0                7             MOV     P3,A    ; Set Port 3 as output for 7 segment display
                         8
000A 7400                9             MOV     A,#00H  ; RED,GREEN
000C F9                 10             MOV     R1,A    ; Set R1 initial state to 000 state
                        11                             ; There will be 8 state in R1 from 000 to 111
                        12                             ; Will be decoded to TRAFFIC signal
                        13
000D 74F7               14             MOV     A,#0F7H ; YELLOW
000F FA                 15             MOV     R2,A    ; Set R2 initial state to 0111 state
                        16                             ; There will be 4 state in R2
                        17                             ; Sequence:
                        18                             ; 0111 => 1011 => 1101 => 1110 => 0111
                        19                             ; Will be updated using Rotator function
                        20
0010 7408               21             MOV     A,#8    ; Set R5 initially point to 8th location of DPTR since there is only 1 DPTR a
0012 FD                 22             MOV     R5,A
                        23
0013 900065             24             MOV     DPTR,#TRAFFIC_AND_SEG   ; Data pointer for Traffic signal and 7 Segment Display toget
                        25
                        26
0016 75F0FF             27     START:  MOV     B,#0FFH
0019 85F0A0             28             MOV     P2,B            ; Clear Yellow Light output
                        29
001C 112A               30             ACALL   UPDATE_TRAFFIC  ; Green Light and Red Light Signal
                        31             ;ACALL  DELAY           ; Delay for Green Light and Red Light
001E 1150               32             ACALL   COUNTDOWN5      ; Countdown from 5
                        33
0020 112F               34             ACALL   UPDATE_R1       ; ACALL UPDATE_R1 for next Traffic
0022 112A               35             ACALL   UPDATE_TRAFFIC  ; Off Green Light, and be ready for Yellow Signal
                        36
0024 1136               37             ACALL   ROTATE_R2       ; Call Yellow Light
                        38
0026 112F               39             ACALL   UPDATE_R1       ; ACALL UPDATE_R1 for next Traffic
                        40
0028 0116               41             AJMP    START
                        42
                        43
                        44     UPDATE_TRAFFIC: ; Function to update and output Traffic signal
002A E9                 45                     MOV     A,R1            ; Selector input for decoder
002B 93                 46                     MOVC    A,@A+DPTR       ; Decode to Traffic signal
002C F590               47                     MOV     P1,A            ; Traffic Light Signal
002E 22                 48                     RET
                        49
                        50     UPDATE_R1:      ; Function to update R1
002F E9                 51                     MOV     A,R1
0030 04                 52                     INC     A                       ; Keep adding R1 by 1
0031 20E30F             53                     JB      A.3,UPDATE_R1_END       ; Change back to 000 state if become 1000
0034 F9                 54                     MOV     R1, A                   ; Update the value of R1
0035 22                 55                     RET
                        56
                        57
                        58     ROTATE_R2:      ; Function to update R2
                        59                     ; Follow the sequence: ROTATE RIGHT of '1', to let the '0' move
0036 EA                 60                     MOV     A,R2
0037 F5A0               61                     MOV     P2,A
0039 03                 62                     RR      A ; 0111 => 1011 => 1101 => 1110 => 0111
003A 30A002             63                     JNB     P2.0,UPDATE_R2_END;
003D FA                 64                     MOV     R2,A
                        65                     ;ACALL  DELAY
003E 22                 66                     RET
                        67
                        68                     ; Function to wrap around yellow light
003F 74F7               69     UPDATE_R2_END:  MOV     A,#0F7H ; Wrap around 0111 => 1110
0041 FA                 70                     MOV     R2, A   ; Update the value of R2
0042 22                 71                     RET
                        72
                        73                     ; Function to wrap around R&G light
0043 7400               74     UPDATE_R1_END:  MOV     A,#00H  ; Wrap around 0-7 of A
0045 F9                 75                     MOV     R1, A   ; Update the value of R1
0046 22                 76                     RET
                        77
                        78                     ; Function to delay for 1 seconds
0047 78FF               79     DELAY:          MOV     R0,#0FFh
0049 79F0               80     DELAY1:         MOV     R1,#0F0h
004B D9FE               81     DELAY2:         DJNZ    R1,DELAY2 ;Decrement register1 and jump to delay2 if it is not 0
004D D8FA               82                     DJNZ    R0,DELAY1 ;Decrement register0 and jump to delay1 if it is not 0
004F 22                 83                     RET
                        84
0050 740E               85     COUNTDOWN5:     MOV     A,#14           ; Set initial R5 point to digit 5
0052 FD                 86                     MOV     R5,A
0053 115F               87                     ACALL   TIMER           ; Count down
0055 115F               88                     ACALL   TIMER           ; Count down
0057 115F               89                     ACALL   TIMER           ; Count down
0059 115F               90                     ACALL   TIMER           ; Count down
005B 7400               91                     MOV     A,#00H
005D F5B0               92                     MOV     P3,A    ; off 7 segment display
                        93
                        94
005F 1D                 95     TIMER:  DEC     R5
0060 ED                 96             MOV     A,R5            ;read input value of R5
                        97             ;ACALL  DELAY
0061 93                 98             MOVC    A,@A+DPTR       ;load value from table
0062 F5B0               99             MOV     P3,A            ;output value
0064 22                100             RET
                       101
                       102
                       103             ; Decoder to Green Light and Red Light Display for 4 Traffic
                       104             ; G1R1 G2R2 G3R3 G4R4
                       105             ; 0 means output
                       106             ; 1 means no output
                       107
                       108             ; Traffic 1
                       109             ; 01 10 10 10 => 6AH    G1 is turned on while other R keep turned on
                       110             ; 11 10 10 10 => 0EAH   G1 is turned off for Y while other R keep turned on
                       111
                       112             ; Traffic 2
                       113             ; 10 01 10 10 => 9AH    G2 is turned on while other R keep turned on
                       114             ; 10 11 10 10 => 0BAH   G2 is turned off for Y while other R keep turned on
                       115
                       116             ; Traffic 3
                       117             ; 10 10 01 10 => 0A6H   G3 is turned on while other R keep turned on
                       118             ; 10 10 11 10 => 0AEH   G3 is turned off for Y while other R keep turned on
                       119
                       120             ; Traffic 4
                       121             ; 10 10 10 01 => 0A9H   G4 is turned on while other R keep turned on
                       122             ; 10 10 10 11 => 0ABH   G4 is turned off for Y while other R keep turned on
                       123
                       124     ;lookup table for 7-segment display pattern
0065 6AEA9ABAA6        125     TRAFFIC_AND_SEG:        DB      6AH,0EAH,9AH,0BAH,0A6H,0AEH,0A9H,0ABH,     3Fh,06h,5Bh,4Fh,66h,6Dh,7D
     AEA9AB3F06
     5B4F666D7D
     077F6F
                       126                     END
